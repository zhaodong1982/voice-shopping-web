# Gemini API Key
GEMINI_API_KEY=your_gemini_api_key_here

# Alipay Sandbox Configuration
# Get these from: https://open.alipay.com/develop/sandbox/app
ALIPAY_APP_ID=your_sandbox_app_id
ALIPAY_PRIVATE_KEY=your_sandbox_private_key
ALIPAY_PUBLIC_KEY=your_sandbox_alipay_public_key

# Alipay Gateway (Default is Sandbox)
ALIPAY_GATEWAY=https://openapi-sandbox.dl.alipaydev.com/gateway.do

# Callbacks
ALIPAY_RETURN_URL=http://localhost:3000/alipay/callback
ALIPAY_NOTIFY_URL=http://localhost:3000/api/alipay/notify
